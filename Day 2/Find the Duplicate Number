Time Limit Exceeded

class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        int n= nums.size();
        int x;
        for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
          
            if (nums[i] == nums[j]) {
                x = nums[i];
            }
        }
    }
    return x;
    }
};

